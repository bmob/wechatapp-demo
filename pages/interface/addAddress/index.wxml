<import src="../../../style/header.wxml" />
<import src="../../../style/footer.wxml" />

<template is="head" />

<view class="page__bd">
  <form bindsubmit="formSubmit" report-submit='true'>
    <view class="weui-cells weui-cells_after-title">
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label">纬度</view>
        </view>
        <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请输入纬度,例：113.405" name="latitude" />
          </view>
      </view>
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label">经度</view>
        </view>
        <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请输入经度,例：23.05203" name="longitude" />
          </view>
      </view>
    </view>
    <view class="weui-btn-area">
            <button class="weui-btn" type="primary" formType="submit">确定</button>
        </view>
  </form>
</view>

<template is="foot" />

